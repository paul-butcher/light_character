{
    "origin": [
        "#[this_characteristic:#characteristic#]tweet#"
    ],
    "tweet": [
        "#this_characteristic# http://example.com?characteristic=#this_characteristic#"
    ],
    "characteristic": [
        "#type_without_period# #colour#",
        "#type_with_period# #colour# #short_period#",
        "#type_with_group##group# #colour# #long_period#"
    ],
    "colour":["R", "G", "W", "Y"],
    "group": ["(#very_low_number#)", "(#very_low_number#+#very_low_number#)"],
    "short_period":["#low_number#s"],
    "long_period":["#high_number#s"],
    "type_with_group":["Fl.", "Oc.", "L.Fl"],
    "type_with_period":["#type_with_group#", "Iso."],
    "type_without_period":["F.", "Q."],
    "very_low_number":["1","2","3","4","5"],
    "low_number":["3","5","8","10","12"],
    "high_number":["10","12","15","18","20"]
}
